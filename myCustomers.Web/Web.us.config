<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="Environment" xdt:Transform="Remove" xdt:Locator="Match(key)" />
    <add key="Region" value="US" xdt:Locator="Match(key)" xdt:Transform="SetAttributes" />
    <add key="Subsidiary" value="US" xdt:Locator="Match(key)" xdt:Transform="SetAttributes" />
  </appSettings>
  <system.web>
    <machineKey xdt:Transform="Remove" />
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>
