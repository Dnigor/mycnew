@model EditCustomerViewModel
<div class="control-group">
  <label class="control-label">@Resources.GetString("ADDCUSTOMER_CONTACTBY")</label>
  <div class="controls">
    @foreach (var method in Model.ContactMethods)
    {
    <label class="checkbox">
      <input data-bind="checked: preferredContactMethods" type="checkbox" value="@method.Key" />&nbsp;@method.Value
    </label>
    }
  </div>
</div>
